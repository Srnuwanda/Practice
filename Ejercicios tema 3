public class OpenBootSantiago {

    public static void main(String[] args) {

        int a=5;
        int b=7;
        int c=9;

        int resultado=suma(a, b, c);

        System.out.println("El resultado es "+resultado);
    }


    public static int suma (int a, int b, int c) {

    return a+b+c;
}
}
